{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "jsx": "react-jsx",
    "strict": false,
    "types": [
      "vite/client",
      "@testing-library/jest-dom",
      "node"
    ],
    "noEmit": true,
    "allowImportingTsExtensions": true
  },
  "include": [
    "src/renderer",
    "src/common"
  ]
}